<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="initial-scale=1.0 , minimum-scale=1.0 , maximum-scale=1.0" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block Footer %}
            <div id="Footer-social-logo">
                <div>
                    <img id="Image-footer-insta" src="{{ asset('Directory/Image-footer-insta.png') }}" alt="Image-footer-insta">
                    <img id="Image-footer-Facbook" src="{{ asset('Directory/Image-footer-Facbook.png') }}" alt="Image-footer-Facbook">
                    <img id="Image-footer-linkedin" src="{{ asset('Directory/Image-footer-linkedin.png') }}" alt="Image-footer-linkedin">
                    <img id="Image-footer-Mail" src="{{ asset('Directory/Image-footer-Mail.png') }}" alt="Image-footer-Mail">
                </div>
                <div>
                    <img id="Image-footer-Sport-multisport-santé" src="{{ asset('Directory/Image-footer-Sport-multisport-santé.png') }}" alt="Image-footer-Sport-multisport-santé">
                </div>
            </div>
            <div id="Footer-link-legal">
                <a id="Mention-legales" href="{{path('app_home')}}"> <h2>Mention legales</h2></a>
                <a id="Cookies" href="{{path('app_home')}}"> <h2>Cookies</h2></a>
                <a id="politique-confidentialité" href="{{path('app_home')}}"> <h2>politique de confidentialité</h2></a>
            </div>
        {% endblock %}
    </body>
</html>
